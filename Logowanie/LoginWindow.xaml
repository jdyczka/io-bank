<Window x:Class="Logowanie.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Logowanie"
        mc:Ignorable="d"
        Title="Logowanie" Height="300" Width="300" ResizeMode="CanMinimize" KeyDown="Window_KeyDown">
    <Grid>
        <Button x:Name="loginButton" Content="Zaloguj" HorizontalAlignment="Left" Margin="105,221,0,0" VerticalAlignment="Top" Width="75" Click="loginButton_Click"/>
        <Button x:Name="exitButton" Content="Wyjdź" HorizontalAlignment="Left" Margin="195,221,0,0" VerticalAlignment="Top" Width="75" Click="exitButton_Click"/>
        <TextBox x:Name="loginTextBox" HorizontalAlignment="Left" Height="26" Margin="96,56,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="151"/>
        <Label x:Name="loginLabel" Content="Email" HorizontalAlignment="Left" Margin="10,56,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.524,0.46" Width="68"/>
        <Label Content="Hasło" HorizontalAlignment="Left" Margin="10,102,0,0" VerticalAlignment="Top" Width="68" RenderTransformOrigin="0.486,0.533"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="96,102,0,0" VerticalAlignment="Top" Width="151" Height="26"/>
    </Grid>
</Window>
