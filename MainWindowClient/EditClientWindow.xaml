            <TextBox x:Name="BuildingNumberBox" HorizontalAlignment="Left" Height="23" Margin="118,230,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="160" PreviewTextInput="NumberValidationTextBox"/>
            <TextBox x:Name="ApartamentNumberBox" HorizontalAlignment="Left" Height="23" Margin="118,258,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="159"/>

